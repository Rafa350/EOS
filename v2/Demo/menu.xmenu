                                            <commandItem title="Steps" displayFormat="" command="CMD_SET_YNUMSTEPS" />
                                            <commandItem title="Units" displayFormat="" command="CMD_SET_YNUMUNITS" />
                                            <exitItem title="Back" />
                                        </menu>
                                    </menuItem>
                                    <exitItem title="Back" />
                                </menu>
                            </menuItem>

                            <!--Configuracio del eix Z -->
                            <menuItem title="Z-Axis...">
                                <menu title="Z-AXIS">
                                    <menuItem title="Limits..">
                                        <menu title="Z-AXIS LIMITS">
                                            <commandItem title="Jerk" displayFormat="" command="CMD_SET_ZJERK" />
                                            <commandItem title="Accel" displayFormat="" command="CMD_SET_ZMAXACCEL" />
                                            <commandItem title="Speed" displayFormat="" command="CMD_SET_ZMAXSPEED" />
                                            <exitItem title="Back" />
                                        </menu>
                                    </menuItem>
                                    <menuItem title="Scale...">
                                        <menu title="Z-AXIS SCALE">
                                            <commandItem title="Steps" displayFormat="" command="CMD_SET_ZNUMSTEPS" />
                                            <commandItem title="Units" displayFormat="" command="CMD_SET_ZNUMUNITS" />
