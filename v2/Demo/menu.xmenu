                                            <commandItem title="TIMEOUT" displayFormat="" command="CMD_APPLYTOUCH_TIMEOUT" />
                                            <exitItem title="Back" />
                                        </menu>
                                    </menuItem>
                                    <menuItem title="APPLY BY TRIGGER...">
                                        <menu title="APPLY BY TRIGGER">
                                            <!-- Retard al inici de cicle -->
                                            <commandItem title="PRE-DELAY" displayFormat="" command="CMD_APPLYTRIGGER_PRE_DELAY" />
                                            <!-- Retard al final de cicle -->
                                            <commandItem title="POST-DELAY" displayFormat="" command="CMD_APPLYTRIGGER_POST_DELAY"/>
