<?xml version="1.0" encoding="utf-8"?>
<resources>
    <menuResource resourceId="MnuMain">
        <menu title="MENU">
            <menuItem title="Configuration...">
                <menu title="CONFIGURATION">
                
                    <!-- Opcions de configuracio dels eixos -->
                    <menuItem title="Axis...">
                        <menu title="AXIS">
                        
