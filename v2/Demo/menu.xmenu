                            <exitItem title="Back" />
                        </menu>
                    </menuItem>
                    <exitItem title="Back" />
                </menu>
            </menuItem>
            <menuItem title="Service...">
                <menu title="SERVICE">
                    <commandItem title="Reset defaults" displayFormat="" command="CMD_RESET_DEFAULTS" />
                    <exitItem title="Exit" />
                </menu>
            </menuItem>
            <commandItem title="Exit" command="CMD_EXIT"/>
