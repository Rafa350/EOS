                                        </menu>
                                    </menuItem>
                                    <exitItem title="Back" />
                                </menu>
                            </menuItem>

                            <exitItem title="Back" />
                        </menu>
                    </menuItem>
                    
                    <menuItem title="Switches...">
                        <menu title="SWITCHES">
                            <commandItem title="Program" displayFormat="" command="CMD_CFG_PROGRAM" />
                            <menuItem title="Steps...">
                                <menu title="STEPS">
                                    <menuItem title="Print label...">
                                        <menu title="PRINT LABEL">
                                            <!-- Retard al inici de cicle -->
                                            <commandItem title="PRE-DELAY" displayFormat="" command="CMD_PRINT_PRE_DELAY" />
                                            <!-- Retard al final de cicle -->
                                            <commandItem title="POST-DELAY" displayFormat="" command="CMD_PRINT_POST_DELAY"/>
                                            <!-- Indica si cal esperar una seyal externa per imprimir -->
                                            <commandItem title="WAIT" displayFormat="" command="CMD_PRINT_WAIT_MODE"/>
                                            <!-- Indica si el buit es actiu durant la impresio -->
                                            <commandItem title="VACUUM" displayFormat="" command="CMD_PRINT_VACUUM_MODE" />
                                            <!-- Temps maxim del cicle, se es sobrepasa es genera un error -->
                                            <commandItem title="TIMEOUT" displayFormat="" command="CMD_PRINT_TIMEOUT"/>
                                            <exitItem title="Back" />
                                       </menu>
                                    </menuItem>
                                    <menuItem title="Arm up...">
