                                            <commandItem title="DETECT TOUCH" displayFormat="" command="CMD_APPLYTRIGGER_TOUCHRETURN"/>
                                            <exitItem title="Back" />
                                        </menu>
                                    </menuItem>
                                    <exitItem title="Back" />
                                </menu>
                            </menuItem>
                            <exitItem title="Back" />
