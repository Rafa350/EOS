digraph {
    ratio = "fill"
    size = "8.3,11.7!"
    margin = 0.5
    fontname = "arial"
    fontsize = 10

    node [
        fontname = "arial",
        fontsize = 14,
        shape = "none",
        margin = 0
    ]

    edge [
